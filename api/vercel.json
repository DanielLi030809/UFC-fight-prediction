{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api.py"
      }
    ],
    "excludeFiles": [
      "**/*.ipynb",
      "tempCodeRunnerFile.py",
      "database.py",
      "models/**",
      "**/__pycache__/**",
      "*.pyc"
    ]
}